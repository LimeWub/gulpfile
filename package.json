{
	"name": "gulpbddel",
	"version": "0.0.1",
	"description": "Compiler/Work setup",
	"main": "gulpfile.js",
	"author": "Myrto Kontouli",
	"credit": "Andrew Welch, nystudio107 <andrew@nystudio107.com>",
	"copyright": "MCK",
	"license": "UNLICENSED",
	"private": true,
	"browserslist": "> 1%, Last 2 versions, iOS 8",
	"babel": {
		"presets": [
			[
				"env",
				{
					"targets": {
						"browsers": [
							"> 1%, Last 2 versions, iOS 8"
						]
					}
				}
			]
		]
	},
	"paths": {
		"src": {
			"base": "./www/site/templates/frontend/src/",
			"fontello": "./www/site/templates/frontend/src/fontello/",
			"fonts": "./www/site/templates/frontend/src/fonts/",
			"js": "./www/site/templates/frontend/src/scripts/",
			"img": "./www/site/templates/frontend/src/img/",
			"css": "./www/site/templates/frontend/src/styles/",
			"scss": "./www/site/templates/frontend/src/styles/scss/"
		},
		"dist": {
			"base": "./www/site/templates/",
			"css": "./www/site/templates/styles",
			"js": "./www/site/templates/scripts",
			"fonts": "./www/site/templates/fonts",
			"img": "./www/site/templates/img"
		},
		"build": {
			"base": "./www/site/templates/frontend/build/",
			"css": "./www/site/templates/frontend/build/styles",
			"fontello": "./www/site/templates/frontend/build/fonts/fontello/",
			"fonts": "./www/site/templates/frontend/build/fonts/",
			"js": "./www/site/templates/frontend/build/scripts/",
			"html": "./www/site/templates/frontend/build/html/"
		},
		"favicon": {
			"src": "./www/site/templates/img/favicon/favicon.ico",
			"dest": "./www/site/templates/img/favicon/",
			"path": "./www/site/templates/img/favicon/"
		},
		"scss": [],
		"templates": "./www/site/templates/"
	},
	"urls": {
		"live": "https://gulpbddel.com/",
		"local": "https://gulpbddel.dev/",
		"critical": "https://gulpbddel.com/"
	},
	"vars": {
		"siteCssName": "site.combined.min.css",
		"scssName": "style.scss",
		"cssName": "style.css"
	},
	"globs": {
		"distCss": [
			"./node_modules/normalize.css/normalize.css",
			"./node_modules/flexboxgrid/dist/flexboxgrid.min.css",
			"./www/site/templates/frontend/build/fonts/fontello/css/fontello-codes.css",
			"./www/site/templates/frontend/build/styles/*.css",
			"./www/site/templates/frontend/src/styles/*.css"
		],
		"fonts": [
			"./www/site/templates/frontend/build/fonts/fontello/font/*.{eot,ttf,woff,woff2}",
			"./www/site/templates/frontend/src/fonts/*.{eot,ttf,woff,woff2}"
		],
		"critical": [
			{
				"url": "",
				"template": "index"
			},
			{
				"url": "blog/stop-using-htaccess-files-no-really",
				"template": "blog/_entry"
			},
			{
				"url": "blog/stop-using-htaccess-files-no-really",
				"template": "blog/_amp_entry"
			},
			{
				"url": "blog",
				"template": "blog/index"
			},
			{
				"url": "blog",
				"template": "blog/amp_index"
			},
			{
				"url": "offline",
				"template": "offline"
			},
			{
				"url": "404",
				"template": "404"
			}
		],
		"download": [
			{
				"url": "https://www.google-analytics.com/analytics.js",
				"dest": "./www/site/templates/scripts/"
			}
		],
		"distJs": [
			"./build/js/*.js",
			"./node_modules/lazysizes/lazysizes.min.js",
			"./node_modules/lazysizes/plugins/bgset/ls.bgset.min.js"
		]
	},
	"dependencies": {
		"fg-loadcss": "^1.2.0",
		"fontfaceobserver": "^2.0.5",
		"lazysizes": "^2.0.6",
		"loadjs": "^3.3.1",
		"normalize.css": "^5.0.0",
		"tiny-cookie": "^1.0.1"
	},
	"devDependencies": {
		"browser-sync": "^2.18.13",
		"babel-plugin-transform-runtime": "^6.15.0",
		"babel-preset-env": "^1.6.0",
		"critical": "0.8.0",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^3.1.0",
		"gulp-babel": "^6.1.2",
		"gulp-cached": "^1.1.1",
		"gulp-concat": "^2.6.0",
		"gulp-cssnano": "^2.1.2",
		"gulp-debug": "^2.1.2",
		"gulp-download": "^0.0.1",
		"gulp-favicons": "^2.2.6",
		"gulp-filter": "^5.0.1",
		"gulp-fontello": "^0.4.6",
		"gulp-header": "^1.8.7",
		"gulp-if": "^2.0.1",
		"gulp-imagemin": "^3.1.1",
		"gulp-load-plugins": "^1.3.0",
		"gulp-newer": "^1.2.0",
		"gulp-plumber": "^1.1.0",
		"gulp-print": "^2.0.1",
		"gulp-rename": "^1.2.2",
		"gulp-replace": "0.5.4",
		"gulp-rev": "^7.1.0",
		"gulp-sass": "^2.1.0",
		"gulp-size": "^2.1.0",
		"gulp-sourcemaps": "^2.2.1",
		"gulp-streamify": "1.0.2",
		"gulp-uglify": "^1.5.4",
		"moment": "^2.14.1",
		"pa11y": "^4.11.0"
	},
	"scripts": {
		"start": "gulp",
		"build": "gulp build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LimeWub/gulpfile/tree/nystudio107_edit"
	},
	"bugs": {
		"email": "myrto.kontouli@999design.com"
	}
}
