{
	"name": "gulpbddel",
	"version": "0.0.1",
	"description": "Compiler/Work setup",
	"main": "gulpfile.js",
	"author": "Myrto Kontouli",
	"credit": "Andrew Welch, nystudio107 <andrew@nystudio107.com>",
	"copyright": "MCK",
	"license": "UNLICENSED",
	"private": true,
	"browserslist": [
		"> 1%",
		"Last 2 versions",
		"iOS 8"
	],
	"babel": {
		"presets": [
			[
				"env",
				{
					"useBuiltIns": true,
					"debug": true
				}
			]
		]
	},
	"paths": {
		"src": {
			"base": "./www/site/templates/_dev/src/",
			"fontello": "./www/site/templates/_dev/src/fonts/fontello/",
			"fonts": "./www/site/templates/_dev/src/fonts/",
			"js": "./www/site/templates/_dev/src/scripts/",
			"img": "./www/site/templates/_dev/src/img/",
			"css": "./www/site/templates/_dev/src/styles/",
			"scss": "./www/site/templates/_dev/src/styles/scss/"
		},
		"dist": {
			"base": "./www/site/templates/",
			"css": "./www/site/templates/styles",
			"js": "./www/site/templates/scripts",
			"fonts": "./www/site/templates/fonts",
			"img": "./www/site/templates/img"
		},
		"build": {
			"base": "./www/site/templates/_dev/build/",
			"css": "./www/site/templates/_dev/build/styles",
			"fontello": "./www/site/templates/_dev/build/fonts/fontello/",
			"fonts": "./www/site/templates/_dev/build/fonts/",
			"js": "./www/site/templates/_dev/build/scripts/",
			"html": "./www/site/templates/_dev/build/html/"
		},
		"favicon": {
			"src": "./www/site/templates/img/favicon/favicon.ico",
			"dest": "./www/site/templates/img/favicon/",
			"path": "./www/site/templates/img/favicon/"
		},
		"scss": [],
		"templates": "./www/site/templates/"
	},
	"urls": {
		"live": "https://gulpbddel.com/",
		"local": "http://gulpbddel.dev/",
		"critical": "https://gulpbddel.com/"
	},
	"vars": {
		"siteCssName": "main.min.css",
		"scssName": "main.scss",
		"cssName": "main.css"
	},
	"globs": {
		"distCss": [
			"./node_modules/normalize.css/normalize.css",
			"./www/site/templates/_dev/build/fonts/fontello/css/fontello-codes.css",
			"./www/site/templates/_dev/build/styles/*.css",
			"./www/site/templates/_dev/src/styles/*.css"
		],
		"fonts": [
			"./www/site/templates/_dev/build/fonts/fontello/font/*.{eot,ttf,woff,woff2}",
			"./www/site/templates/_dev/src/fonts/*.{eot,ttf,woff,woff2}"
		],
		"critical": [
			{
				"url": "",
				"template": "index"
			},
			{
				"url": "blog/stop-using-htaccess-files-no-really",
				"template": "blog/_entry"
			},
			{
				"url": "blog/stop-using-htaccess-files-no-really",
				"template": "blog/_amp_entry"
			},
			{
				"url": "blog",
				"template": "blog/index"
			},
			{
				"url": "blog",
				"template": "blog/amp_index"
			},
			{
				"url": "offline",
				"template": "offline"
			},
			{
				"url": "404",
				"template": "404"
			}
		],
		"download": [
			{
				"url": "https://www.google-analytics.com/analytics.js",
				"dest": "./www/site/templates/scripts/"
			}
		],
		"distJs": [
			"./www/site/templates/_dev/build/scripts/*.js",
			"./node_modules/lazysizes/lazysizes.min.js",
			"./node_modules/lazysizes/plugins/bgset/ls.bgset.min.js"
		]
	},
	"dependencies": {
		"fg-loadcss": "^1.3.1",
		"fontfaceobserver": "^2.0.13",
		"lazysizes": "^4.0.0-rc3",
		"loadjs": "^3.5.1",
		"normalize.css": "^7.0.0",
		"tiny-cookie": "^2.0.2"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"browser-sync": "^2.18.13",
		"critical": "^0.9.1",
		"fancy-log": "^1.3.0",
		"git-rev-sync": "^1.9.1",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^4.0.0",
		"gulp-babel": "^7.0.0",
		"gulp-cached": "^1.1.1",
		"gulp-concat": "^2.6.1",
		"gulp-cssnano": "^2.1.2",
		"gulp-debug": "^3.1.0",
		"gulp-download": "^0.0.1",
		"gulp-favicons": "^2.2.7",
		"gulp-filter": "^5.0.1",
		"gulp-fontello": "^0.4.6",
		"gulp-header": "^1.8.9",
		"gulp-if": "^2.0.2",
		"gulp-imagemin": "^3.3.0",
		"gulp-livereload": "^3.8.1",
		"gulp-load-plugins": "^1.5.0",
		"gulp-newer": "^1.3.0",
		"gulp-plumber": "^1.1.0",
		"gulp-print": "^2.0.1",
		"gulp-rename": "^1.2.2",
		"gulp-replace": "^0.6.1",
		"gulp-rev": "^8.0.0",
		"gulp-sass": "^3.1.0",
		"gulp-size": "^2.1.0",
		"gulp-sourcemaps": "^2.6.1",
		"gulp-streamify": "^1.0.2",
		"gulp-uglify": "^3.0.0",
		"moment": "^2.18.1",
		"pa11y": "^4.12.1"
	},
	"scripts": {
		"start": "gulp",
		"build": "gulp build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LimeWub/gulpfile/tree/nystudio107_edit"
	},
	"bugs": {
		"email": "myrto.kontouli@999design.com"
	}
}
